  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#FFBDFE"
      }
    },
    {
      "scope": [
        "meta.paragraph.markdown",
        "string.other.link.description.title.markdown"
      ],
      "settings": {
        "foreground": "#FAF0FF"
      }
    },
    {
      "scope": [
        "entity.name.section.markdown",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#905BBE"
      }
    },
    {
      "scope": [
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#FF80D2"
      }
    },
    {
      "scope": ["markup.quote.markdown"],
      "settings": {
        "foreground": "#FF80D2"
      }
    },
    {
      "scope": [
        "markup.bold.markdown",
        "punctuation.definition.bold.markdown"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F957FF"
      }
    },
    {
      "scope": [
        "markup.italic.markdown",
        "punctuation.definition.italic.markdown"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#F7B6C2"
      }
    },
    {
      "scope": ["punctuation.definition.metadata.markdown"],
      "settings": {
        "foreground": "#f3b8c2"
      }
    },
    {
      "scope": [
        "markup.underline.link.image.markdown",
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#FA6BEB"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#63034B"
      }
    },
    {
      "name": "Quotes",
      "scope": "punctuation.definition.string",
      "settings": {
        "foreground": "#F5449F"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#FDC2FF"
      }
    },
    {
      "name": "String Quoted",
      "scope": ["string.quoted", "variable.other.readwrite.js"],
      "settings": {
        "fontStyle": "",
        "foreground": "#FDC2FF"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FF05BC"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#FF05BC"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#A170C6"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "punctuation.definition.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#F490D6"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#FF80D2"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#EEA5E7"
      }
    },

    {
      "name": "JavaScript Other Variable",
      "scope": "variable.other.object.js",
      "settings": {
        "foreground": "#F5007B"
      }
    },
    {
      "name": "TypeScript[React] Variables and Object Properties",
      "scope": [
        "variable.other.readwrite.alias.ts",
        "variable.other.readwrite.alias.tsx",
        "variable.other.readwrite.ts",
        "variable.other.readwrite.tsx",
        "variable.other.object.ts",
        "variable.other.object.tsx",
        "variable.object.property.ts",
        "variable.object.property.tsx",
        "variable.other.ts",
        "variable.other.tsx",
        "variable.tsx",
        "variable.ts"
      ],
      "settings": {
        "foreground": "#F5007B"
      }
    },
    {
      "name": "TypeScript Classes",
      "scope": "meta.class entity.name.type.class.tsx",
      "settings": {
        "foreground": "#d29ffcff"
      }
    },
    {
      "name": "TypeScript Entity Name Type",
      "scope": ["entity.name.type.tsx", "entity.name.type.module.tsx"],
      "settings": {
        "foreground": "#d29ffcff"
      }
    },
    {
      "name": "TypeScript Method Declaration e.g. `constructor`",
      "scope": [
        "meta.method.declaration storage.type.ts",
        "meta.method.declaration storage.type.tsx"
      ],
      "settings": {
        "foreground": "#E6A2D4"
      }
    },
    {
      "name": "Variable Property Other object property",
      "scope": ["variable.other.object.property"],
      "settings": {
        "foreground": "#F7B6C2"
      }
    },
    {
      "name": "Variable Instances",
      "scope": [
        "variable.instance",
        "variable.other.instance",
        "variable.readwrite.instance",
        "variable.other.readwrite.instance",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#AC80DB"
      }
    },
    {
      "name": "JavaScript Variable Other ReadWrite",
      "scope": ["variable.other.readwrite.js", "variable.parameter"],
      "settings": {
        "foreground": "#E0D7DE"
      }
    },
    {
      "name": "Template Strings",
      "scope": "string.template meta.template.expression",
      "settings": {
        "foreground": "#c63ed3"
      }
    },
    {
      "name": "Backtics(``) in Template Strings",
      "scope": "string.template punctuation.definition.string",
      "settings": {
        "foreground": "#F599FF"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#FA6BEB"
      }
    },
    {
      "name": "Keywords and Storage types",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#F599FF" //E100FA F599FF
      }
    },
    {
      "name": "Keywords operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#F599FF"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "meta.var.expr",
        "meta.class meta.method.declaration meta.var.expr storage.type.js",
        "storage.type.property.js",
        "storage.type.property.ts"
      ],
      "settings": {
        "foreground": "#c792ea"
      }
    },
    {
      "name": "JavaScript module imports and exports",
      "scope": [
        "variable.other.meta.import.js",
        "meta.import.js variable.other",
        "variable.other.meta.export.js",
        "meta.export.js variable.other"
      ],
      "settings": {
        "foreground": "#FDC2FF"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#F7B6C2"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "",
        "foreground": "#CF4FD8"
      }
    },
    {
      "name": "Variables, Let and Const",
      "scope": ["variable.other.readwrites", "meta.definition.variable"],
      "settings": {
        "fontStyle": "",
        "foreground": "#F7B6C2"
      }
    },
    {
      "name": "Support Variable Property",
      "scope": "support.variable.property",
      "settings": {
        "foreground": "#AC80DB"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#F486D3"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#E0D7DE",
        "fontStyle": ""
      }
    },

    {
      "scope": [
        "support.function",
        "entity.name.function",
        "meta.function-call",
        "meta.function",
        "meta.method.declaration",
        "meta.function-call support",
        "meta.decorator meta.definition.method entity.name.function",
        "meta.decorator meta.method.declaration entity.name.function",
        "variable.language.super.ts",
        "source.directive",
        "meta.function-call.generic",
        "meta.method-call.static.php",
        "meta.method-call.php",
        "meta.class storage.type",
        "meta.method.groovy",
        "meta.bracket.square.access",
        "entity.name.function-call.elixir",
        "punctuation.output.liquid support.variable.liquid",
        "meta.function.echo.edge source.js keyword.operator.error-control.js"
      ],
      "settings": { "foreground": "#FF29F1" }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FA6BEB"
      }
    },
    {
      "name": "Entity Name Type",
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#d29ffc"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#F7B6C2"
      }
    },
    {
      "name": "Meta - Decorator",
      "scope": ["punctuation.decorator"],
      "settings": {
        "foreground": "#F7B6C2"
      }
    },
    {
      "name": "Punctuation/Brackets/Tags",
